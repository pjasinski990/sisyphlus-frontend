{
  "name": "sisyphlus-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "start": "vite preview",
    "lint": "eslint . --ext ts,tsx --cache --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --cache --report-unused-disable-directives --max-warnings 0 --fix",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "diagram:classes": "./generate_class_diagram.sh",
    "diagram:deps": "./generate_deps_diagram.sh",
    "diagram:all": "pnpm run diagram:classes && npm run diagram:deps"
  },
  "dependencies": {
    "@tanstack/query-core": "^5.85.5",
    "@tanstack/react-query": "^5.85.3",
    "framer-motion": "^12.12.2",
    "lucide-react": "^0.511.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.6.2",
    "tinykeys": "^3.0.0",
    "uuid": "^11.1.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@eslint/js": "^9.26.0",
    "@stylistic/eslint-plugin": "^5.2.3",
    "@tailwindcss/postcss": "^4.1.7",
    "@tailwindcss/vite": "^4.1.11",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/mime-types": "^3.0.0",
    "@types/node": "^20",
    "@types/pdf-parse": "^1.1.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-v8": "3.1.3",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-boundaries": "^5.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jsdom": "^26.1.0",
    "tailwind-scrollbar": "^4.0.2",
    "tailwindcss": "^4.1.7",
    "typescript": "^5.9.2",
    "vite": "^6.3.5",
